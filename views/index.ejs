    <link rel="stylesheet" href="/bootstrap-switch/dist/css/bootstrap3/bootstrap-switch.min.css" />

    <link rel="stylesheet" href="/css/index.css" />
    <link rel="stylesheet" href="/css/splash.css" />
    <link rel="stylesheet" href="/css/feed.css"/>
</head>

<body ng-controller="mainController">
    <div class="navbar navbar-default nav-links navbar-fixed-top">
        <div class="container-fluid">
            <a class="navbar-brand hidden-sm hidden-xs" ui-sref="feed">
                <img src="/images/logo/HashtagSell_Logo_80px.svg" class="logo-nav mini-navbar" />
            </a>
            <a class="navbar-brand visible-sm visible-xs" ng-click="toggleOffCanvasSideNav()" style="cursor: pointer">
                <i class="fa fa-bars fa-2x"></i>
            </a>
            <div class="col-lg-5 col-lg-offset-2 col-md-5 col-md-offset-1 col-sm-5 col-sm-offset-1 col-xs-7 awesomeBar" ng-controller="awesomeBarController">
                <form role="search" ng-submit="awesomeBarSubmit()">
                    <div class="input-group">
                        <input
                            type="text"
                            class="form-control"
                            ng-model="query"
                            placeholder="I'm looking to buy...">
                            <!--mentio-->
                            <!--mentio-require-leading-space="true"-->
                            <!--mentio-macros="macros"-->
                            <!--mentio-id="'hashtag'"-->
                            <!--mentio-typed-term="typedTerm"-->

                            <!--popover='*Hint: We search in your area by default.  If you want to search in another city use the @ symbol.'-->
                            <!--popover-placement="bottom"-->
                            <!--popover-trigger="focus"-->

                        <span class="input-group-btn">
                            <button type="submit" class="btn btn-danger" type="button">
                                <span class="glyphicon glyphicon-search"></span>
                            </button>
                        </span>
                        </input>
                    </div>
                </form>


                <!--<mentio-menu style="z-index: 1100 !important;"-->
                             <!--mentio-for="'hashtag'"-->
                             <!--mentio-trigger-char="'@'"-->
                             <!--mentio-items="places"-->
                             <!--mentio-template-url="/place-mentions.tpl"-->
                             <!--mentio-search="searchPlaces(term)"-->
                             <!--mentio-select="getPlacesTextRaw(item)">-->
                <!--</mentio-menu>-->


            </div>
            <ul class="nav navbar-nav pull-right">
                <li ng-controller="newPostController">
                    <button type="button" class="btn btn-danger" ng-click="newPost(); $event.stopPropagation();">#Sell It</button>
                </li>
                <li class="dropdown hidden-xs" ng-controller="userMenu">
                    <a href class="dropdown-toggle" on-toggle="toggled(open)">
                        <img class='img-circle' ng-src={{userObj.user_settings.profile_photo}} width="38"/>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-right" ng-if="!userObj.user_settings.loggedIn" ng-cloak>
                        <li><a href ng-click="signIn()">Sign In</a></li>
                        <li><a href ng-click="signUp()">Create Account</a></li>
                    </ul>
                    <ul class="dropdown-menu dropdown-menu-right" ng-if="userObj.user_settings.loggedIn" ng-cloak>
                        <li><a ui-sref="settings.general">User Settings</a></li>
                        <li class="divider"></li>
                        <li><a href="#" ng-click="logout()">Logout</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>


    <div class="master-container">
        <div class="overlay" ng-class="{ 'active' : !sideNavOffCanvas.hidden}" ng-click="toggleOffCanvasSideNav()"></div>
        <div class="sidebar-container" ng-class="{ 'active' : !sideNavOffCanvas.hidden}">
            <div class="bs-profile-nav sidebar-offcanvas">
                <div ng-controller="sideProfile" class="profile" style="background-image : url({{userObj.user_settings.banner_photo}}); background-size : cover; cursor: pointer;" ui-sref="profile">
                    <div class="profileCircle">
                        <img class='bs-profile-image' ng-src={{userObj.user_settings.profile_photo}} width="70px;" height="70px;"/>
                    </div>
                </div>
                <div ng-controller="newPostController" class="sellButton" ui-sref="profile" style="cursor: pointer">
                    <div class="bs-username ng-cloak" ng-show="userObj.user_settings.name"> @{{userObj.user_settings.name}}</div>
                    <div class="bs-username ng-cloak" ng-show="!userObj.user_settings.name">
                        <a href ng-click="signIn(); $event.stopPropagation();">Sign In</a> /
                        <a href ng-click="signUp(); $event.stopPropagation();">Create Account</a>
                    </div>
                    <button type="submit" class="btn btn-danger btn-sm btn-block" ng-click="newPost(); $event.stopPropagation();">#Sell It</button>
                </div>

                <!--SideNav-->
                <div class="list-group bs-side-nav ng-cloak" ng-controller="sideNav.controller">
                    <a ng-repeat="item in sideNav.items" ui-sref="{{::item.link}}" class="list-group-item" ng-class="item.active ? 'active' : 'null'">
                        {{::item.name}}
                        <span class="badge">{{item.alerts}}</span>
                    </a>
                </div>


                <!--FILTER BAR ONLY VISIBLE WHEN USER ON SEARCH RESULTS PAGE-->
                <div ng-controller="filterBar">
                    <div class="list-group ng-cloak" ng-show="showFilterBar">
                        <a class="list-group-item ng-cloak">
                            <span class="pull-left">{{rangeSlider.rangeValue[0] | currency : $ : 0}}</span>
                            <span class="pull-right">{{rangeSlider.rangeValue[1] | currency : $ : 0}}</span>
                            <span slider ng-model="rangeSlider.rangeValue" min="rangeSlider.min" max="rangeSlider.max" value="rangeSlider.rangeValue" range="true" formater="myFormatter" on-stop-slide="slideDelegate(value)"></span>
                        </a>
                        <!--<input slider ng-model="value" type="text" id="mySlider1"  options="ngSliderOptions" />-->
                        <a class="list-group-item ng-cloak">
                            <input bs-switch ng-model="filterToggles.mustHavePrice" type="checkbox" switch-on-color="info" switch-on-text="Yes" switch-off-color="primary" switch-off-text="No" switch-label="Require price" style="min-width: 220px;">
                        </a>

                        <a class="list-group-item ng-cloak">
                            <input bs-switch ng-model="filterToggles.mustHaveImage" type="checkbox" switch-on-color="info" switch-on-text="Yes" switch-off-color="primary" switch-off-text="No" switch-label="Require image" style="min-width: 220px;">
                        </a>

                        <a class="list-group-item ng-cloak">
                            <input bs-switch ng-model="views.showMap" type="checkbox" switch-on-color="info" switch-on-text="Show" switch-off-color="primary"  switch-off-text="Hide" switch-label="Map" style="min-width: 220px;">
                        </a>

                        <a class="list-group-item">
                            <span class="btn-group">
                                <label class="btn btn-default" ng-model="views.gridView" btn-radio="true"><i class="fa fa-th"></i></label>
                                <label class="btn btn-default" ng-model="views.gridView" btn-radio="false"><i class="fa fa-list"></i></label>
                            </span>
                        </a>
                    </div>
                </div>
            </div>
        </div>


        <div class="content-container" ng-class="{ 'offcanvas-open' : !sideNavOffCanvas.hidden}">
            <div class="container">
                <div ui-view></div>
            </div>
        </div>
    </div>


